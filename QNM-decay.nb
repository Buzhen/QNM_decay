Cell[TextData[{
 "Equations take from Wolfram MathWorld [1]\nCoulomb Wave Functions are \
defined using confluent hypergeometric function of the 1st kind given in [2]\n\
\nCoulomb Wave function of the 1st kind is\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["F", "L"], "(", 
     RowBox[{"\[Eta]", ",", "\[Rho]"}], ")"}], "=", 
    RowBox[{
     RowBox[{
      SubscriptBox["C", "L"], "(", "\[Eta]", ")"}], 
     SuperscriptBox["\[Rho]", 
      RowBox[{"L", "+", "1"}]], 
     SubscriptBox[
      SuperscriptBox["e", 
       RowBox[{
        RowBox[{"-", "i"}], " ", "\[Rho]"}]], "1"], 
     RowBox[{
      SubscriptBox["F", "1"], "(", 
      RowBox[{
       RowBox[{"L", "+", "1", "-", 
        RowBox[{"i", " ", "\[Eta]"}]}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"2", " ", "L"}], "+", "2"}], ";", " ", 
        RowBox[{"2", " ", "i", " ", "\[Rho]"}]}]}], ")"}]}]}], 
   TraditionalForm]],ExpressionUUID->"78d7222a-7353-4920-be0c-85d48feb4841"],
 "\nwhere\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["C", "L"], "(", "\[Eta]", ")"}], "=", 
    FractionBox[
     RowBox[{
      RowBox[{
       SuperscriptBox["2", "L"], 
       SuperscriptBox["e", 
        RowBox[{
         RowBox[{"-", "\[Pi]"}], " ", 
         RowBox[{"\[Eta]", " ", "/", " ", "2"}]}]]}], "|", 
      RowBox[{"\[CapitalGamma]", "(", 
       RowBox[{"L", "+", "1", "+", 
        RowBox[{"i", " ", "\[Eta]"}]}], ")"}], "|"}], 
     RowBox[{"\[CapitalGamma]", "(", 
      RowBox[{
       RowBox[{"2", "L"}], "+", "1"}], ")"}]]}], TraditionalForm]],
  ExpressionUUID->"dd000f12-c827-4d02-8b46-03428255a96f"],
 "\nand\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["\[Placeholder]", "1"], 
    RowBox[{
     SubscriptBox["F", "1"], "(", 
     RowBox[{"a", ";", "b", ";", "z"}], ")"}]}], TraditionalForm]],
  ExpressionUUID->"21b90334-9db7-4052-961e-08d859a2d563"],
 "\nis the confluent hypergeometric function of the 1st kind\n\nCoulomb Wave \
function of the 2nd kind is\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["G", "L"], "(", 
     RowBox[{"\[Eta]", ",", " ", "\[Rho]"}], ")"}], "=", 
    RowBox[{
     RowBox[{
      FractionBox[
       RowBox[{"2", "\[Eta]"}], 
       RowBox[{
        SuperscriptBox[
         SubscriptBox["C", "0"], "2"], "(", "\[Eta]", ")"}]], 
      RowBox[{
       RowBox[{
        SubscriptBox["F", "L"], "(", 
        RowBox[{"\[Eta]", ",", " ", "\[Rho]"}], ")"}], "[", 
       RowBox[{
        RowBox[{"ln", "(", 
         RowBox[{"2", "\[Rho]"}], ")"}], "+", 
        FractionBox[
         RowBox[{
          SubscriptBox["q", "L"], "(", "\[Eta]", ")"}], 
         RowBox[{
          SubscriptBox["p", "L"], "(", "\[Eta]", ")"}]]}], "]"}]}], "+", 
     RowBox[{
      FractionBox["1", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"2", "L"}], "+", "1"}], ")"}], 
        RowBox[{
         SubscriptBox["C", "L"], "(", "\[Eta]", ")"}]}]], 
      SuperscriptBox["\[Rho]", 
       RowBox[{"-", "L"}]], 
      RowBox[{
       SuperscriptBox[
        SubscriptBox["\[Sum]", 
         RowBox[{"K", "=", 
          RowBox[{"-", "L"}]}]], "\[Infinity]"], 
       RowBox[{
        RowBox[{
         SuperscriptBox[
          SubscriptBox["a", "k"], "L"], "(", "\[Eta]", ")"}], 
        SuperscriptBox["\[Rho]", 
         RowBox[{"K", "+", "L"}]]}]}]}]}]}], TraditionalForm]],ExpressionUUID->
  "a6b07d3c-26e9-4d49-8ce4-136d6499b691"],
 "\nwhere\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      SubscriptBox["C", "0"], "2"], "(", "\[Eta]", ")"}], "=", 
    RowBox[{"2", "\[Pi]", " ", 
     SuperscriptBox[
      RowBox[{"\[Eta]", "(", 
       RowBox[{
        SuperscriptBox["e", 
         RowBox[{"2", "\[Pi]", " ", "\[Eta]"}]], "-", "1"}], ")"}], 
      RowBox[{"-", "1"}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "a7dd238a-ea96-4fc4-befc-40cac5d5715c"],
 "\nand ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["q", "L"], ",", " ", 
    SubscriptBox["p", "L"]}], TraditionalForm]],ExpressionUUID->
  "c1bd7ec7-51c3-431a-9211-9d05dd6dd85b"],
 " and ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox[
    SubscriptBox["a", "k"], "L"], TraditionalForm]],ExpressionUUID->
  "6952efa2-6d3d-4cb2-8cd2-f197496e2f45"],
 " are defined in [3] as\n",
 Cell[BoxData[{
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["p", "L"], "(", "\[Eta]", ")"}], " ", "=", " ", 
    RowBox[{"2", " ", "\[Eta]", 
     FractionBox[
      RowBox[{
       RowBox[{
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "+", 
           SuperscriptBox["\[Eta]", "2"]}], ")"}], 
         RowBox[{"(", 
          RowBox[{"4", "+", 
           SuperscriptBox["\[Eta]", "2"]}], ")"}]}], "..."}], 
       RowBox[{"(", 
        RowBox[{
         SuperscriptBox["L", "2"], "+", 
         SuperscriptBox["\[Eta]", "2"]}], ")"}], 
       SuperscriptBox["2", 
        RowBox[{"2", "L"}]]}], 
      SuperscriptBox[
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"2", "L"}], "+", "1"}], ")"}], "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"2", "L"}], ")"}], "!"}], "]"}], "2"]]}]}], 
   TraditionalForm], "\[IndentingNewLine]", 
  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{
      SubscriptBox["q", "L"], "(", "\[Eta]", ")"}], 
     RowBox[{
      SubscriptBox["p", "L"], "(", "\[Eta]", ")"}]], "=", 
    RowBox[{
     RowBox[{
      SuperscriptBox[
       SubscriptBox["\[Sum]", 
        RowBox[{"s", "=", "1"}]], "L"], 
      FractionBox["s", 
       RowBox[{
        SuperscriptBox["s", "2"], "+", 
        SuperscriptBox["\[Eta]", "2"]}]]}], "-", 
     RowBox[{
      SuperscriptBox[
       SubscriptBox["\[Sum]", 
        RowBox[{"s", "=", "1"}]], 
       RowBox[{
        RowBox[{"2", "L"}], "+", "1"}]], 
      FractionBox["1", "s"]}], "+", 
     RowBox[{"Re", "[", 
      FractionBox[
       RowBox[{
        RowBox[{"\[CapitalGamma]", "'"}], 
        RowBox[{"(", 
         RowBox[{"1", "+", 
          RowBox[{"i", " ", "\[Eta]"}]}], ")"}]}], 
       RowBox[{"\[CapitalGamma]", "(", 
        RowBox[{"1", "+", 
         RowBox[{"i", " ", "\[Eta]"}]}], ")"}]], "]"}], "+", 
     RowBox[{"2", "\[Gamma]"}], "+", 
     FractionBox[
      RowBox[{
       SubscriptBox["r", "L"], "(", "\[Eta]", ")"}], 
      RowBox[{
       SubscriptBox["p", "L"], "(", "\[Eta]", ")"}]]}]}], TraditionalForm]}],
  ExpressionUUID->"9622e4f1-c963-4041-b44b-9b9f9266c3f0"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["r", "L"], "(", "\[Eta]", ")"}], "=", 
    RowBox[{
     FractionBox[
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"-", "1"}], ")"}], 
       RowBox[{"L", "+", "1"}]], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"2", "L"}], ")"}], "!"}]], 
     RowBox[{"Im", "[", 
      RowBox[{
       RowBox[{
        RowBox[{
         FractionBox["1", 
          RowBox[{
           RowBox[{"2", "L"}], "+", "1"}]], "+", 
         FractionBox[
          RowBox[{"2", 
           RowBox[{"(", 
            RowBox[{"i\[Eta]", "-", "L"}], ")"}]}], 
          RowBox[{"2", 
           RowBox[{"L", "(", 
            RowBox[{"1", "!"}], ")"}]}]], "+", 
         FractionBox[
          RowBox[{
           SuperscriptBox["2", "2"], 
           RowBox[{"(", 
            RowBox[{"i\[Eta]", "-", "L"}], ")"}], 
           RowBox[{"(", 
            RowBox[{"i\[Eta]", "-", "L", "+", "1"}], ")"}]}], 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{"2", "L"}], "-", "1"}], ")"}], 
           RowBox[{"(", 
            RowBox[{"2", "!"}], ")"}]}]], "+"}], "..."}], "+", 
       FractionBox[
        RowBox[{
         RowBox[{
          RowBox[{
           SuperscriptBox["2", 
            RowBox[{"2", "L"}]], 
           RowBox[{"(", 
            RowBox[{"i\[Eta]", "-", "L"}], ")"}], 
           RowBox[{"(", 
            RowBox[{"i\[Eta]", "-", "L", "+", "1"}], ")"}]}], "..."}], 
         RowBox[{"(", 
          RowBox[{"i\[Eta]", "+", "L", "-", "1"}], ")"}]}], 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"2", "L"}], ")"}], "!"}]]}], "]"}]}]}], TraditionalForm]],
  ExpressionUUID->"fe277281-76dd-4486-b190-c9dc41c6e72c"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      SubscriptBox["a", 
       RowBox[{"-", "L"}]], "L"], "=", "1"}], ";", " ", 
    RowBox[{
     SuperscriptBox[
      SubscriptBox["a", 
       RowBox[{"L", "+", "1"}]], "L"], "=", "0"}], ";", " ", 
    RowBox[{
     SuperscriptBox[
      SubscriptBox["a", "k"], "L"], "=", 
     FractionBox[
      RowBox[{
       RowBox[{"2", "\[Eta]", " ", 
        SuperscriptBox[
         SubscriptBox["a", 
          RowBox[{"k", "-", "1"}]], "L"]}], "-", 
       SuperscriptBox[
        SubscriptBox["a", 
         RowBox[{"k", "-", "2"}]], "L"], "-", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{
          RowBox[{"2", "k"}], "-", "1"}], ")"}], 
        RowBox[{
         SubscriptBox["p", "L"], "(", "\[Eta]", ")"}], 
        SuperscriptBox[
         SubscriptBox["A", "k"], "L"]}]}], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"k", "-", "L", "-", "1"}], ")"}], 
       RowBox[{"(", 
        RowBox[{"k", "+", "L"}], ")"}]}]]}]}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d3f41ad5-5502-4c7b-a469-5df47616e6d2"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      SubscriptBox["A", 
       RowBox[{"L", "+", "1"}]], "L"], "=", "1"}], ";", " ", 
