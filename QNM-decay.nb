      RowBox[{
       SubscriptBox["\[PartialD]", "\[Theta]"], 
       RowBox[{"(", 
        RowBox[{"sin\[Theta]", 
         RowBox[{
          SubscriptBox["\[PartialD]", "\[Theta]"], 
          SuperscriptBox[
           SubscriptBox["S", "lm"], 
           RowBox[{"(", "s", ")"}]]}]}], ")"}]}]}], "+", 
     RowBox[{
      RowBox[{"[", 
       RowBox[{
        RowBox[{
         FractionBox[
          RowBox[{
           SuperscriptBox["a", "2"], 
           SuperscriptBox["\[Omega]", "2"]}], 
          SuperscriptBox["c", "2"]], 
         SuperscriptBox["cos", "2"], "\[Theta]"}], "-", 
        FractionBox[
         SuperscriptBox["m", "2"], 
         RowBox[{
          SuperscriptBox["sin", "2"], "\[Theta]"}]], "-", 
        RowBox[{
         FractionBox[
          RowBox[{"2", "sa\[Omega]"}], "c"], "cos\[Theta]"}], "-", 
        RowBox[{"2", "sm", " ", 
         FractionBox["cos\[Theta]", 
          RowBox[{
           SuperscriptBox["sin", "2"], "\[Theta]"}]]}], "-", 
        RowBox[{"(", 
         RowBox[{
          RowBox[{
           SuperscriptBox["s", "2"], 
           SuperscriptBox["ctg", "2"], "\[Theta]"}], "-", "s"}], ")"}], "+", 
        RowBox[{
         SuperscriptBox[
          SubscriptBox["A", "lm"], 
          RowBox[{"(", "s", ")"}]], "(", "\[Omega]", ")"}]}], "]"}], 
      SuperscriptBox[
       SubscriptBox["S", "lm"], 
       RowBox[{"(", "s", ")"}]]}]}], "=", "0"}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "b5e7f630-1b20-4df2-8204-36f1145dcac6"],
 "\nwith the solutions:\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SuperscriptBox[
      SubscriptBox["S", "lm"], 
      RowBox[{"(", "s", ")"}]], "(", "\[Theta]", ")"}], "=", 
    RowBox[{
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{"-", "1"}], ")"}], "s"], 
     SqrtBox[
      FractionBox[
       RowBox[{
        RowBox[{"2", "l"}], "+", "1"}], 
       RowBox[{"4", "\[Pi]"}]]], 
     RowBox[{
      SuperscriptBox[
       SubscriptBox["d", 
        RowBox[{"m", ",", 
         RowBox[{"-", "s"}]}]], "l"], "(", "\[Theta]", ")"}]}]}], 
   TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "5bf69078-dac9-4fc5-826c-99f04dffcbed"],
